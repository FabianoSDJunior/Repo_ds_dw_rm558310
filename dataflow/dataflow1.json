{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ORIGEM",
						"type": "DatasetReference"
					},
					"name": "trabalhofuturocaracteristicas"
				},
				{
					"dataset": {
						"referenceName": "ORIGEM",
						"type": "DatasetReference"
					},
					"name": "pesquisatrabalhofuturo"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DESTINO",
						"type": "DatasetReference"
					},
					"name": "Destino"
				}
			],
			"transformations": [
				{
					"name": "join2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id_Pesquisa as short,",
				"          Estado as string,",
				"          Cidade as string,",
				"          Genero as string,",
				"          Idade as short,",
				"          Profissao as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> trabalhofuturocaracteristicas",
				"source(output(",
				"          Id_Pesquisa as short,",
				"          Data_Pesquisa as string,",
				"          Satisfacao_Atual as short,",
				"          Equilibrio_Vida_Trabalho as short,",
				"          Saude_Mental_Status as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> pesquisatrabalhofuturo",
				"trabalhofuturocaracteristicas, pesquisatrabalhofuturo join(trabalhofuturocaracteristicas@Id_Pesquisa == pesquisatrabalhofuturo@Id_Pesquisa,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Destino"
			]
		}
	}
}